<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="102dp"
        app:srcCompat="@drawable/logo_menu" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="130dp"
        app:srcCompat="@drawable/catalogo_boton" />

    <Button
        android:id="@+id/catalogo_boton"
        android:layout_width="358dp"
        android:layout_height="121dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="125dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="260dp"
        app:srcCompat="@drawable/subiranuncio_boton" />

    <Button
        android:id="@+id/subirAnuncio_boton"
        android:layout_width="358dp"
        android:layout_height="121dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="255dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="390dp"
        app:srcCompat="@drawable/bookmarks_boton" />

    <Button
        android:id="@+id/bookmarks_boton"
        android:layout_width="358dp"
        android:layout_height="121dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="385dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="520dp"
        app:srcCompat="@drawable/perfil_boton" />

    <Button
        android:id="@+id/perfil_boton"
        android:layout_width="358dp"
        android:layout_height="121dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="515dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="650dp"
        app:srcCompat="@drawable/toolbar_background" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="330dp"
        android:layout_height="81dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="50dp"

        app:srcCompat="@drawable/toolbar" />

    <Button
        android:id="@+id/toolbar_main_boton"
        android:layout_width="71dp"
        android:layout_height="62dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="620dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        android:layout_marginBottom="14dp" />

    <Button
        android:id="@+id/toolbar_chat_boton"
        android:layout_width="43dp"
        android:layout_height="63dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="140dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="620dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        android:layout_marginBottom="9dp" />

    <Button
        android:id="@+id/toolbar_faq_boton"
        android:layout_width="43dp"
        android:layout_height="63dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="224dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="620dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        android:layout_marginBottom="9dp" />

    <Button
        android:id="@+id/toolbar_acercade_boton"
        android:layout_width="71dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="302dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="620dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/transparent"
        android:layout_marginBottom="12dp"
        />

</RelativeLayout>